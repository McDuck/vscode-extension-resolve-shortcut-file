{
  "name": "resolve-shortcut-file",
  "displayName": "Resolve Shortcut File",
  "publisher": "your-name",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-name/resolve-shortcut-file"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "activationEvents": [
    "onCustomEditor:lnk.resolver"
  ],
  "contributes": {
    "customEditors": [
      {
        "viewType": "lnk.resolver",
        "displayName": "Resolve Shortcut File",
        "selector": [
          {
            "filenamePattern": "*.lnk"
          }
        ]
      }
    ]
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./",
    "test": "node ./out/test/runTest.js",
    "pretest": "npm run compile"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.85.0",
    "@vscode/test-electron": "^2.5.2",
    "glob": "^13.0.0",
    "mocha": "^11.7.5",
    "typescript": "^5.3.3"
  }
}
